{
  "name": "duzyid",
  "version": "1.0.0",
  "description": "The DuzyID ads system that allows you to integrate ads to your videos.",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/ts-node src/server.ts",
    "build": "./node_modules/.bin/tsc",
    "prod": "sh -c \"export $(echo $(grep -v '^#' .env | xargs -0)) && node bin/server.js\"",
    "in-docker": "node bin/server.js",
    "test:mocha": "./node_modules/mocha/bin/_mocha --exit --require ts-node/register -- --recursive ./src/test/**/**/*.*.spec.ts",
    "test:compile": "tsc -p tsconfig.json",
    "test:full": "npm run test:compile && istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive ./bin/test/**/**/*.*.spec.js",
    "test": "npm run test:full"
  },
  "keywords": [
    "duzy",
    "duzyid"
  ],
  "author": "CodeIT",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/is-url": "^1.2.28",
    "@types/lodash": "^4.14.120",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.21",
    "@types/request-promise-native": "^1.0.15",
    "@types/sinon": "^7.0.8",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.9.0",
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "sinon": "^7.2.7",
    "supertest": "^3.4.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1"
  },
  "dependencies": {
    "express": "^4.16.4",
    "http-status": "^1.3.1",
    "is-url": "^1.2.4",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "nedb-promise": "^2.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "typescript": "^3.3.1",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "week-validator": "^1.3.5"
  }
}